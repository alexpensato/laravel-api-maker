{
    "name": "alexpensato/laravel-api-maker",
    "description": "Automate the generation of your REST APIs",
    "type": "library",
    "license": "MIT",
    "keywords": ["rest", "api", "fractal", "generator", "repository", "bdd"],
    "authors": [
        {
            "name": "Alex Magalhaes",
            "email": "contato@pensato.com.br"
        }
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/alexpensato/laravel-api-maker",
    "require": {
        "php": "^7.1.3",
        "ext-json": "^1.6",
        "illuminate/support": "^5.6",
        "guzzlehttp/guzzle": "^6.3",
        "league/fractal": "^0.17",
        "codeception/specify": "^1.1",
        "codeception/verify": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pensato\\Api\\": "src/"
        }
    }
}
